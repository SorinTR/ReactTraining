{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types":0,
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 2],
        "react/jsx-indent-props": ["error", 2],
        "indent": [2, "tab"],
        "react/prefer-stateless-function": [0, { "ignorePureComponents": true}],
        "linebreak-style": 0,
        
        "no-tabs": 0
    }
}